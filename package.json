{ 
  "homepage": "https://mgsb-dev.github.io/tier-one-staging",
  "name": "tier-one-staging",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "predeploy": "npm run build",
    "deploy": "cd dist && cp index.html 404.html && cd .. && gh-pages -d dist"
  },
  "dependencies": {
    "@types/vue-feather-icons": "^5.0.4",
    "@zhuowenli/vue-feather-icons": "^5.0.2",
    "add-to-calendar-button": "^2.2.9",
    "animate.css": "^4.1.1",
    "execa": "latest",
    "minimist": "^1.2.8",
    "normalize.css": "^8.0.1",
    "sass": "^1.58.3",
    "v-calendar": "^3.0.0-alpha.8",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6",
    "vue-screen-utils": "^1.0.0-beta.13",
    "vue3-rich-accordion": "^0.0.7"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "^18.16.16",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.34.0",
    "eslint-plugin-vue": "^9.9.0",
    "gh-pages": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "typescript": "~4.7.4",
    "vite": "^4.1.3",
    "vue-tsc": "^1.1.5"
  }
}
